{
  "datacenter": "{{datacenter}}",
  "data_dir": "{{data_dir}}",
  "bind_addr": "{{ansible_default_ipv4.address}}",
  "enable_syslog": true,
  "log_level": "debug",
  "retry_join": [
  {% for ip in consul_hosts if ip != ansible_default_ipv4.address %}
    "{{ip}}"{% if not loop.last %},{% endif %}

  {% endfor %}
  ]
}
